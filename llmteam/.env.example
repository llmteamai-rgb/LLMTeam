# LLMTeam Environment Configuration
# Copy this file to .env and configure your settings

# -------------------------------------------------------------------------
# LLMTeam Configuration
# -------------------------------------------------------------------------

# License key (required for enterprise features)
LLMTEAM_LICENSE_KEY=

# API server port
LLMTEAM_PORT=8000

# Logging configuration
LLMTEAM_LOG_FORMAT=json    # json or console
LLMTEAM_LOG_LEVEL=INFO     # DEBUG, INFO, WARNING, ERROR

# -------------------------------------------------------------------------
# Database Configuration
# -------------------------------------------------------------------------

# PostgreSQL connection string (optional)
# DATABASE_URL=postgresql://llmteam:llmteam_secret@postgres:5432/llmteam

# -------------------------------------------------------------------------
# Redis Configuration
# -------------------------------------------------------------------------

# Redis connection URL
REDIS_URL=redis://redis:6379/0
REDIS_PORT=6379

# -------------------------------------------------------------------------
# PostgreSQL Configuration (for docker-compose)
# -------------------------------------------------------------------------

POSTGRES_USER=llmteam
POSTGRES_PASSWORD=llmteam_secret
POSTGRES_DB=llmteam
POSTGRES_PORT=5432

# -------------------------------------------------------------------------
# LLM Provider API Keys (configure as needed)
# -------------------------------------------------------------------------

# OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...
# AZURE_OPENAI_API_KEY=...
# AZURE_OPENAI_ENDPOINT=https://....openai.azure.com
